<!DOCTYPE html>
<html>
<head>
  <title>Expecto</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if logged_in? %>
<%= link_to 'Sign Out', session_url, method: :delete %>
<% end %>

<%= yield %>

</body>
</html>
